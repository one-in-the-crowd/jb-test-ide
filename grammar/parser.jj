options {
	STATIC = false ;
}
PARSER_BEGIN(Adder)
package com.jetbrains.drob.parser;
class Adder {
    public static void main( String[] args ) throws ParseException, TokenMgrError {
        Adder parser = new Adder( System.in ) ;
        int result = parser.Start() ;
        System.out.println("Result: "+result);
    }
}
PARSER_END(Adder)



SKIP : { " " }
SKIP : { "\n" | "\r" | "\r\n" }
TOKEN : { < PLUS : "+" > }
TOKEN : { < NUMBER : (["0"-"9"])+ > }



int Start() :
{
    Token token;
    int tempNumber;
    int number;
}
{
	token = <NUMBER>
	{tempNumber = Integer.parseInt(token.image);}
	{number = tempNumber;}
	(
		<PLUS>
		token = <NUMBER>
		{tempNumber = Integer.parseInt(token.image);}
		{number += tempNumber;}
	)*
	<EOF>
	{return number;}
}
